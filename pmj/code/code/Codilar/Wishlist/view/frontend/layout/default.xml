<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="top.links">
        <block class="Codilar\Wishlist\Block\Link" name="wish-list-link" after="my-account-link">
            <arguments>
                <argument name="sortOrder" xsi:type="number">60</argument>
            </arguments>
        </block>
        </referenceBlock>
        <referenceBlock name="top.search">
            <block class="Magento\Framework\View\Element\Template" name="advanced-search-link" template="Codilar_Wishlist::advanced/link.phtml" />
        </referenceBlock>
    </body>
</page>
